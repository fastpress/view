{
    "name": "fastpress/view",
    "description": "A flexible and intuitive templating engine for PHP, optimized for use with the Fastpress framework. Provides an easy-to-use interface for rendering views, handling layouts, and managing content blocks.",
    "type": "library",
    "version": "0.1.3",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "phpcbf --standard=PSR12 src/",
        "lint": "phpcs --standard=PSR12 src/",
        "analyze": "phpstan analyse src/"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fastpress\\Presentation\\": "src/Presentation/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Samson Daniel",
            "email": "samayo@pm.me"
        }
    ],
    "keywords": ["templating", "php", "fastpress", "view", "layout", "render"],
    "homepage": "https://github.com/fastpress/view",
    "support": {
        "issues": "https://github.com/fastpress/view/issues",
        "source": "https://github.com/fastpress/view"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
